{
	"name": "engelbart-document",
	"version": "1.0.0",
	"description": "Augmenting Human Intellect: A Conceptual Framework - Interactive Document with TypeScript",
	"main": "Augmenting-Human-Intellect.html",
	"scripts": {
		"start": "http-server",
		"serve": "http-server -p 3000 -o Augmenting-Human-Intellect.html",
		"dev": "concurrently \"npm run watch\" \"npm run serve-dev\"",
		"serve-dev": "http-server -p 3000 -o Augmenting-Human-Intellect.html -c-1",
		"build": "tsc",
		"watch": "tsc --watch",
		"type-check": "tsc --noEmit",
		"clean": "rimraf assets/js"
	},
	"keywords": [
		"engelbart",
		"typescript",
		"interactive-document",
		"douglas-engelbart",
		"human-intellect"
	],
	"author": "TJ Labs",
	"license": "ISC",
	"devDependencies": {
		"concurrently": "^8.2.0",
		"http-server": "^14.1.1",
		"rimraf": "^5.0.1",
		"typescript": "^5.1.6"
	}
}